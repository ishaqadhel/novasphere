{
  "name": "novasphere",
  "version": "1.0.0",
  "description": "Construction Supplier Management System",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "migrate": "node src/script/database/migrate/index.js",
    "seed": "node src/script/database/seed/index.js",
    "etl": "node src/script/database/etl/index.js",
    "db:reset": "npm run migrate && npm run seed",
    "db:full-reset": "npm run migrate && npm run seed && npm run etl",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "prepare": "husky"
  },
  "keywords": [
    "construction",
    "supplier",
    "management",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "hjs": "^0.0.6",
    "moment": "^2.30.1",
    "mysql2": "^3.11.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "prettier": "^3.4.2"
  }
}
